{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/axios",
      "@types/node",
      "@nuxtjs/vuetify",
      "@nuxtjs/toast",
      "nuxt-moment-jalaali",
      "@nuxtjs/pwa",
      "@nuxtjs/auth-next",
      "@nuxtjs/proxy",
      "@types/loadsh",
      "@types/jest",
      "@nuxt/content",
      "index.d.ts"
    ]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist",
    "cypress"
  ]
}
